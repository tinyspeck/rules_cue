load(
    "//cue:cue.bzl",
    "cue_instance",
    "cue_vet_test",
)

cue_instance(
    name = "shelf",
    srcs = ["shelf.cue"],
    ancestor = "//test/testdata/library/cue.mod",
    visibility = ["//test/testdata/library:__subpackages__"],
    deps = ["//test/testdata/library/cue.mod/pkg/examplebooks.com/books:book"],
)

cue_vet_test(
    name = "vet_fiction_shelf_failing",
    files_to_vet = ["fiction_shelf_failing.json"],
    schema = ":shelf",
)

cue_vet_test(
    name = "vet_fiction_shelf_passing",
    files_to_vet = ["fiction_shelf_passing.json"],
    schema = ":shelf",
)